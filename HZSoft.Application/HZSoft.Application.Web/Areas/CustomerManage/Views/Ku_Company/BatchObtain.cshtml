@using (Html.BeginForm("BatchObtain", "Ku_Company", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <script src="~/Content/scripts/jquery/jquery-1.8.3.min.js"></script>
    <h2>
        批量领取
    </h2>
    <div>
        <fieldset id="myfieldset">
            <legend>excel模版格式 </legend><font color="red">领取数据的模板格式如下，若模板格式不正确，则相应的数据不能领取!</font><br />
            <p style="color: Red; text-align: center;">@Html.ActionLink("下载模版", "GetFile")</p>
        </fieldset>
    </div>
    <div style="margin-top: 20px;">
        <fieldset id="myfieldset1">
            <legend>批量信息领取</legend>
            <p>
                选择文件：<input id="FileUpload" type="file" name="files" style="width: 250px; height: 24px;
                background: White" class="easyui-validatebox" />
            </p>
            <p>
                <input id="btnImport" type="submit" value="批量领取" onclick="importIng()" style="width: 80px; height: 28px;" />
            </p>
            <div style="color: Red; text-align: center;width: 80px; height: 250px;">@ViewBag.error</div>
        </fieldset>
    </div>
    <script>
        function importIng() {
            $("#btnImport").val("正在领取");
        }

    </script>
}