@using (Html.BeginForm("Import", "LOHAS_Customer", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <script src="~/Content/scripts/jquery/jquery-1.8.3.min.js"></script>
    <style>
        body {
            font-size: 14px;
        }
    </style>
    <div>
        <fieldset id="myfieldset">
            <legend>excel模版格式 </legend><font color="red">导入的模板格式如下，若模板格式不正确，则相应的不能导入!</font><br />
            <p style="color: Red; text-align: center;">@Html.ActionLink("下载模版", "GetFile")</p>
        </fieldset>
    </div>
    <div style="margin-top: 20px;">
        <fieldset id="myfieldset1">
            <legend>批量信息导入</legend>
            <p>
                <input id="FileUpload" type="file" name="files" style="width: 250px; height: 24px;background: White" class="easyui-validatebox" />
                <input id="btnImport" type="submit" value="导入" onclick="importIng()" style="width: 80px; height: 24px;" />
            </p>
            <p style="text-align:center">
                <img class="loadimg" style="display:none" src="~/Content/images/timg.gif" />
            </p>
            <p style="color: Red; text-align: center;">@ViewBag.error</p>
        </fieldset>
    </div>
    <script>
        function importIng() {
            $("#btnImport").val("正在导入");
            $(".loadimg").show();
        }

    </script>
}