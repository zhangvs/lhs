@using (Html.BeginForm("ImportPDD", "ZZT_PDDCustomer", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <script src="~/Content/scripts/jquery/jquery-1.8.3.min.js"></script>
    <h2>
        拼多多订单批量导入
    </h2>
    <div>
        <fieldset id="myfieldset">
            <legend>excel模版格式 </legend><font color="red">导入拼多多订单的模板格式如下，若模板格式不正确，则相应的拼多多订单不能导入!</font><br />
            <p style="color: Red; text-align: center;">@Html.ActionLink("下载模版", "GetFilePDD")</p>
        </fieldset>
    </div>
    <div style="margin-top: 20px;">
        <fieldset id="myfieldset1">
            <legend>拼多多订单批量信息导入</legend>
            <p>
                选择文件：<input id="FileUpload" type="file" name="files" style="width: 250px; height: 24px;
                background: White" class="easyui-validatebox" />
            </p>
            <p>
                <input id="btnImport" type="submit" value="导入" onclick="importIng()" style="width: 80px; height: 28px;" />
            </p>
            <p style="color: Red; text-align: center;">@ViewBag.error</p>
        </fieldset>
    </div>
    <script>
        function importIng() {
            $("#btnImport").val("正在导入");
        }
        
    </script>
}